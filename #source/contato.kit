<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="shortcut icon" href="img/voce/02.png">
    <link rel="stylesheet" href="css/contato.css">

    <title>Bikcraft | Contato</title>
</head>

<body>
    <div class="wrapper">
        <!-- @import "html/header.html" -->
        <main class="content">
            <section class="nosso">
                <div class="nosso__container">
                    <div class="nosso__row">
                        <div class="nosso__column">
                            <div class="nosso__header _anim-items _anim-no-hide">respostas em até 24h</div>
                            <div class="nosso__title title title__white _anim-items _anim-no-hide">
                                nosso contato<span>.</span>
                            </div>
                        </div>
                        <div class="nosso__column">
                            <div class="nosso__sign">
                                <img src="../img/pedale/02.png" alt="">
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <section class="nosso-background"></section>
            <section class="nosso-online">
                <div class="nosso-online__container">
                    <div class="nosso-online__row">
                        <div class="nosso-online__column">
                            <div class="nosso-online__items">
                                <div class="nosso-online__text _anim-items _anim-no-hide">Loja Online</div>
                                <div class="nosso-online__contato-item _anim-items _anim-no-hide">
                                    <div class="nosso-online__icon"><img src="img/nosso-contato/01.png" alt=""></div>
                                    <span>Rua Ali Perto, 35 Rio de Janeiro - RJ Brasil - Terra - Vita Láctea</span>
                                </div>
                                <div class="nosso-online__contato-item _anim-items _anim-no-hide">
                                    <div class="nosso-online__icon"><img src="img/nosso-contato/02.png" alt=""></div>
                                    <span>vendas@bikcraft.com assistencia@bikcraft.com</span>
                                </div>
                                <div class="nosso-online__contato-item _anim-items _anim-no-hide">
                                    <div class="nosso-online__icon"><img src="img/nosso-contato/03.png" alt=""></div>
                                    <span class="number-span">+55 21 9999-9999</span>
                                </div>
                                <div class="nosso-online__social">
                                    <a href="" class="nosso-online__social-item _anim-items _anim-no-hide">
                                        <img src="img/nosso-contato/04.png" alt="">
                                    </a>
                                    <a href="" class="nosso-online__social-item _anim-items _anim-no-hide">
                                        <img src="img/nosso-contato/05.png" alt="">
                                    </a>
                                    <a href="" class="nosso-online__social-item _anim-items _anim-no-hide">
                                        <img src="img/nosso-contato/06.png" alt="">
                                    </a>
                                </div>
                            </div>
                            <div class="nosso-online__sign">
                                <img src="img/nosso-contato/07.png" alt="">
                            </div>
                        </div>
                        <div class="nosso-online__column">
                            <div class="nosso-form">
                                <form action="#" class="nosso-form__body">
                                    <div class="nosso-form__top">
                                        <div class="nosso-form__item nosso-form__input">
                                            <label for="formName" class="nosso-form__label _anim-items _anim-no-hide">
                                                Name
                                            </label>
                                            <input id="formName" autocomplete="off" type="text" name="name"
                                                data-value="Seu nome" class="input req" />
                                        </div>
                                        <div class="nosso-form__item nosso-form__input">
                                            <label for="formPhone" class="nosso-form__label _anim-items _anim-no-hide">
                                                Telefone
                                            </label>
                                            <input id="formPhone" autocomplete="off" type="tel" name="phone"
                                                data-value="+55 (21) 9999-9999" class="input" />
                                        </div>
                                    </div>
                                    <div class="nosso-form__item">
                                        <label for="formEmail" class="nosso-form__label _anim-items _anim-no-hide">
                                            Email
                                        </label>
                                        <input id="formEmail" autocomplete="off" type="text" name="email"
                                            data-value="contato@email.com" class="input req" />
                                    </div>
                                    <div class="nosso-form__item">
                                        <label for="formMessage" class="nosso-form__label _anim-items _anim-no-hide">
                                            Mensagem
                                        </label>
                                        <textarea id="formMessage" autocomplete="off" name="message"
                                            data-value="O que você precisa?" class="input">
                                        </textarea>
                                    </div>
                                    <div class="nosso-form__button _anim-items _anim-no-hide">
                                        <button type="submit" class="btn">
                                            <span>ENVIAR MENSAGEM</span></button>
                                    </div>
                                </form>
                            </div>
                            <div class="nosso-form__sign">
                                <img src="img/nosso-contato/08.png" alt="">
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <section class="locais">
                <div class="locais__container">
                    <div class="locais__title title title__black _anim-items _anim-no-hide">
                        lojas locais<span>.</span>
                    </div>
                    <div class="locais__row">
                        <div class="locais__column">
                            <div class="locais__body">
                                <div class="locais__map ibg _anim-items _anim-no-hide">
                                    <img src="img/locais/01.png" alt="">
                                </div>
                                <div class="locais__item">
                                    <div class="locais__city _anim-items _anim-no-hide"><span></span>Rio de Janeiro
                                    </div>
                                    <div class="locais__contacts">
                                        <div class="locais__addrres _anim-items _anim-no-hide">
                                            Rua Ali Perto, 25
                                            Rio de Janeiro - RJ
                                        </div>
                                        <div class="locais__addrres _anim-items _anim-no-hide">
                                            rio@bikcraft.com
                                            21 9999-9999
                                        </div>
                                    </div>
                                    <div class="locais__time _anim-items _anim-no-hide">
                                        <img src="img/locais/03.png" alt="">
                                        <span>08-18h de seg à dom</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="locais__column">
                            <div class="locais__body">
                                <div class="locais__map ibg _anim-items _anim-no-hide">
                                    <img src="img/locais/02.png" alt="">
                                </div>
                                <div class="locais__item">
                                    <div class="locais__city _anim-items _anim-no-hide"><span></span>São Paulo</div>
                                    <div class="locais__contacts">
                                        <div class="locais__addrres _anim-items _anim-no-hide">
                                            Rua Ali Perto, 25
                                            São Paulo - SP
                                        </div>
                                        <div class="locais__addrres _anim-items _anim-no-hide">
                                            sp@bikcraft.com
                                            11 9999-9999
                                        </div>
                                    </div>
                                    <div class="locais__time _anim-items _anim-no-hide">
                                        <img src="img/locais/03.png" alt="">
                                        <span>08-18h de seg à dom</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </main>
        <!-- @import "html/footer.html" -->
    </div>
    <!-- @import "html/js.html" -->
</body>

</html>